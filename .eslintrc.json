{
    "env": {
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        // "indent": ["error", 2],
        // "quotes": ["error", "double"],
        "constructor-super": "error",
        "no-this-before-super": "error"
    }
}